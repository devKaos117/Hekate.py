{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://github.com/devKaos117/Hekate.py/blob/main/documentation/schema/version.schema.json",
    "title": "Version information",
    "description": "A object with the information about the version found",
    "readOnly": true,
    "type": "object",
    "properties":{
        "current_version": {
            "description": "Current version",
            "type": "string"
        },
        "latest_version": {
            "description": "Latest version found",
            "type": "string"
        },
        "update_found": {
            "description": "indicating if update is available",
            "type": "boolean"
        },
        "source_url": {
            "description": "URL to the source of information",
            "type": "string"
        },
        "release_date": {
            "description": "Release date of the latest version",
            "type": "string"
        },
        "method": {
            "description": "Name of the method that provided the result",
            "type": "string"
        }
    }
}